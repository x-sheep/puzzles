<SettingsFlyout
    x:Class="PuzzleModern.UWP.HelpFlyout"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Help"
    Width="646"
    SizeChanged="HelpFlyout_SizeChanged"
    Loaded="HelpFlyout_Loaded"
    Unloaded="HelpFlyout_Unloaded"
    Padding="0">

    <WebView x:Name="HelpView" NavigationStarting="HelpView_NavigationStarting" NavigationCompleted="HelpView_NavigationCompleted" />
</SettingsFlyout>
